{
  "name": "Focused",
  "description": "Focused uses the scanning reading technique to highlight the important sentences for you. ",
  "version": "0.0.1",
  "manifest_version": 3,
  "background": {
    "service_worker": "src/background.js"
  },
  "permissions": ["activeTab", "scripting"],
  "host_permissions": ["https://*/*", "http://*/*"],
  "action": {
    "default_popup": "src/popup.html",
    "icon":"Smart.png"
  },
  "icons": {
    "512": "Smart.png"
  }
}
